{
  "expo": {
    "name": "Run It",
    "slug": "run-it",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Runnit needs your location to track runs and capture territory."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Run It to access your photos to update your profile picture."
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox",
          "RNMapboxMapsDownloadToken": "pk.eyJ1Ijoia2l2aWpheTA5IiwiYSI6ImNtbGgxMjR5azAwbDIzZ3F3bG9icmtvbmcifQ.bOaVvvXvrxu6HM8RCyggYQ"
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    }
  }
}